<!DOCTYPE html>

<html lang="en">
<head>
    <title>@ViewBag.Title</title>
    <base href="/">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta name="format-detection" content="telephone=no" />
    <!--<link href="https://docs.google.com/uc?id=1YJMurDn2C-Y_z5S1p0mKVteXtVsxgD8q" rel="shortcut icon" type="image/x-icon" />-->
    <link href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" integrity="sha384-wvfXpqpZZVQGK6TAh5PVlGOfQNHSoD2xbE+QkPxCAFlNEevoEH3Sl0sibVcOQVnN" crossorigin="anonymous">
    <link href="~/Content/Style/Site.css" rel="stylesheet">
</head>
<body>
    <main>
        <header>
            <div class="container">
                <div class="navbar-header">
                    <div class="panel-table">
                        <div class="panel-row">
                            <div class="panel-col systeminfo">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </header>
        <section>
            <div class="container">
                @RenderBody()
            </div>                 
        </section>
        <footer>
            <div class="container text-center">
                <span class="font regular gray size14">Copyright <span class="font regular size16"><i class="fa fa-copyright"></i></span> 2019 @(DateTime.Now.Year > 2019 ? (" - " + DateTime.Now.Year) : "") Mahidol University. All rights reserved.</span>
            </div>
        </footer>
    </main>
</body>
</html>